 "css": ".heading{\n    background-color:#f86608;\n    padding:8px 0px;\n    text-align:center;\n    color:white;\n    font-weight:700;\n}\n.blue{\n    background-color:#f86608;\n    color:white;\n}\n.flex-container {\n    display: flex;\n    justify-content: space-between;\n    width: 100%;\n}\n.left-div, .right-div {\n    flex: 1;\n    display: flex;\n}\n.col{\n    display:inline-block;\n}\n.row{\n    display:flex;\n}\ntable, th, td {\n  border: 2px solid black;\n}\ntable{\n    margin-top:20px;\n    table-layout:fixed;\n    width:100%;\n}\n.border{\n    border:2px solid black;\n}\n.text-center{\n    justify-content:center;\n}\np{\n    font-size:10px;\n}\n",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Way Bill",
